#!/bin/bash
set -e

# If the option is configure then enable the site configuration.
if [ "$1" = "configure" ] ; then
    if [ -x "$(which a2ensite)" ] ; then
	a2ensite jpoker || true
    fi
    if [ -x "$(which a2enmod)" ] ; then
	a2enmod proxy_http 
    fi
fi

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
